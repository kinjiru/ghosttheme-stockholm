{
  "name": "stockholm",
  "description": "Fully featured theme with clean presentation.",
  "homepage": "https://hackersandslackers.com",
  "demo": "https://hackersandslackers.com",
  "version": "0.0.1",
  "main": "webpack/global.js",
  "scripts": {
    "clean": "rm dist/bundle.js",
    "develop": "webpack -d --mode development",
    "build": "webpack --mode production",
    "test": "NODE_ENV=/"
  },
  "presets": [
    "env"
  ],
  "author": {
    "name": "Todd Birchard",
    "email": "toddbirchard@gmail.com",
    "url": "https://toddbirchard.com"
  },
  "keywords": [
    "ghost-theme",
    "blog-theme",
    "nodejs",
    "data-science",
    "data-engineering"
  ],
  "engines": {
    "node": "v10.15.0",
    "ghost": ">=2.18.0",
    "ghost-api": "v2"
  },
  "screenshots": {
    "desktop": "https://hackers.nyc3.cdn.digitaloceanspaces.com/img/desktop-screenshot.jpg",
    "mobile": "https://hackers.nyc3.cdn.digitaloceanspaces.com/img/mobile-screenshot.png"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toddbirchard/ghosttheme-stockholm"
  },
  "config": {
    "posts_per_page": 10,
    "image_sizes": {
      "extrasmall": {
        "width": 26
      },
      "small": {
        "width": 370
      },
      "small_retina": {
        "width": 840
      },
      "medium": {
        "width": 700
      },
      "medium_retina": {
        "width": 1400
      },
      "large": {
        "width": 780
      },
      "large_retina": {
        "width": 1560
      },
      "extralarge": {
        "width": 1500
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.4",
    "@babel/runtime-corejs2": "^7.3.4",
    "@sendgrid/mail": "^6.3.1",
    "@types/highlight.js": "^9.12.3",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "baguettebox.js": "^1.11.0",
    "dotenv-webpack": "^1.7.0",
    "fuzzysort": "^1.1.4",
    "ghost-email-service": "^1.2.0",
    "ghost-pagination": "^0.1.3",
    "ghost-search": "^1.0.1",
    "github-profile-card": "^2.0.1",
    "graphql-request": "^1.8.2",
    "highlight.js": "^9.15.6",
    "jquery": "^3.3.1",
    "node-fetch": "^2.3.0",
    "paginate": "^0.2.0",
    "scrollbooster": "^1.1.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "css-declaration-sorter": "4.0.1",
    "css-loader": "^2.1.1",
    "eslint-loader": "^2.1.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "font-config-webpack-plugin": "^1.3.1",
    "html-minifier-webpack-plugin": "^2.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-less": "^3.1.4",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
